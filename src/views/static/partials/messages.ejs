
<% if(messages.error) { %>

  <div class="alert alert-warning alert-dismissible fade show" role="alert" style="position: absolute;top:90%;">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button> Validation errors:
    <ul>

      <% messages.error.forEach((err) => { %>
        <li>
          <%=`${err}`%>
        </li>
        <% }) %>
    </ul>
  </div>
  <% } %>
